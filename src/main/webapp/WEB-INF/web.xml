<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <context-param>
    <param-name>encoding</param-name>
    <param-value>UTF-8</param-value>
  </context-param>
  <context-param>
    <param-name>CaptchaContainer</param-name>
    <param-value>com.my.web.captcha.CookieCaptchaContainer</param-value>
  </context-param>
  <context-param>
    <param-name>Timeout</param-name>
    <param-value>180000</param-value>
  </context-param>
  <context-param>
    <param-name>UserService</param-name>
    <param-value>com.my.user.service.UserServiceImpl</param-value>
  </context-param>
  <context-param>
    <param-name>UserDAO</param-name>
    <param-value>com.my.user.dao.UserDAOMap</param-value>
  </context-param>
</web-app>
