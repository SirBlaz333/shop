<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
  <display-name>Archetype Created Web Application</display-name>
  <context-param>
    <param-name>encoding</param-name>
    <param-value>UTF-8</param-value>
  </context-param>
  <context-param>
    <param-name>CaptchaContainer</param-name>
    <param-value>com.my.web.captcha.container.impl.CookieCaptchaContainer</param-value>
  </context-param>
  <context-param>
    <param-name>Timeout</param-name>
    <param-value>10000</param-value>
  </context-param>
  <context-param>
    <param-name>UserService</param-name>
    <param-value>com.my.service.user.UserServiceImpl</param-value>
  </context-param>
  <context-param>
    <param-name>UserDAO</param-name>
    <param-value>com.my.dao.user.impl.UserDAOMap</param-value>
  </context-param>
</web-app>